{
  "name": "hybrbase-challenge-backend",
  "version": "1.0.0",
  "description": "A NestJS backend for project management with TypeScript",
  "main": "dist/main.js",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "lint": "eslint '{src,test}/**/*.ts'",
    "test": "jest",
    "migration:create": "sequelize migration:generate --name",
    "migration:status": "sequelize db:migrate:status",
    "migration:up": "sequelize db:migrate",
    "migration:down": "sequelize db:migrate:undo --name"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^8.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.4.6",
    "@types/dotenv-safe": "^8.1.6",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.5",
    "dotenv-safe": "^9.1.0",
    "jsonwebtoken": "^9.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.0.0",
    "sequelize": "^6.6.0",
    "socket.io": "^4.0.0",
    "typeorm": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.0.0",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "license": "MIT"
}
